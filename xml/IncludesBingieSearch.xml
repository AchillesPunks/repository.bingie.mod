                    <aligny>center</aligny>
                    <textcolor>$INFO[Skin.String(BingiePrimaryTextColor)]</textcolor>
                    <font>Reg30</font>
                    <height>36</height>
                    <visible>Integer.IsGreater(Container($PARAM[id]).NumItems,0) | Container($PARAM[id]).IsUpdating</visible>
                </control>
                <control type="multiimage">
