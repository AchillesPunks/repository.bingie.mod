    <!-- Search Radio Key -->
    <include name="SearchRadioKey">
        <param name="force_refresh" value="false"/>
        <param name="onclick2Cond" default="!String.IsEmpty(Skin.String(CustomSearchTerm))" />
        <param name="onclick2" default="Skin.SetString(CustomSearchTerm)" />
        <definition>
            <control type="radiobutton" id="$PARAM[id]">
                <height>search_key_size</height>
                <width>search_radiokey_width</width>
                <label/>
                <radioposx>$PARAM[radioposx]</radioposx>
                <radioposy>$PARAM[radioposy]</radioposy>
                <radiowidth>$PARAM[radiowidth]</radiowidth>
                <radioheight>$PARAM[radioheight]</radioheight>
                <textureradioonfocus colordiffuse="$INFO[Skin.String(ButtonFocusTextColor)]">$PARAM[texture]</textureradioonfocus>
                <textureradioofffocus colordiffuse="$INFO[Skin.String(ButtonFocusTextColor)]">$PARAM[texture]</textureradioofffocus>
                <textureradioonnofocus colordiffuse="$INFO[Skin.String(BingiePrimaryTextColor)]">$PARAM[texture]</textureradioonnofocus>
                <textureradiooffnofocus colordiffuse="$INFO[Skin.String(BingiePrimaryTextColor)]">$PARAM[texture]</textureradiooffnofocus>
                <ondown>$PARAM[ondown]</ondown>
                <onclick condition="$PARAM[force_refresh]">SetProperty(CustomSearch,1,home)</onclick>
                <onclick>$PARAM[onclick]</onclick>
                <onclick condition="$PARAM[onclick2Cond]">$PARAM[onclick2]</onclick>
                <include>SearchKeyDefs</include>
            </control>
        </definition>
    </include>

    <!-- Search Key -->
    <include name="SearchKey">
        <param name="size" default="search_key_size" />
        <definition>
            <control type="button" id="$PARAM[id]">
                <width>$PARAM[size]</width>
                <height>$PARAM[size]</height>
                <onup>$PARAM[onup]</onup>
                <ondown>$PARAM[ondown]</ondown>
                <include>SearchKeyDefs</include>
                <label>$PARAM[key]</label>
                <onclick>Skin.SetString(CustomSearchTerm,$INFO[Skin.String(CustomSearchTerm)]$PARAM[key])</onclick>
            </control>
        </definition>
    </include>

    <!-- Native Search Key -->
    <include name="NativeSearchKey">
        <param name="size" default="search_key_size" />
        <definition>
            <control type="button" id="$PARAM[id]">
                <nested />
                <width>$PARAM[size]</width>
                <height>$PARAM[size]</height>
                <onup>$PARAM[onup]</onup>
                <ondown>$PARAM[ondown]</ondown>
                <include>SearchKeyDefs</include>
            </control>
        </definition>
    </include>

    <include name="SearchKeyDefs">
        <align>center</align>
        <font>Reg34</font>
        <textoffsetx>0</textoffsetx>
        <selectedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</selectedcolor>
        <textcolor>$INFO[Skin.String(ButtonTextColor)]</textcolor>
        <focusedcolor>$INFO[Skin.String(ButtonFocusTextColor)]</focusedcolor>
        <disabledcolor>$INFO[Skin.String(GeneralTextColor)]</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="$INFO[Skin.String(ButtonFocusColor)]" border="20">dialogs/keyboard/key-nf.png</texturefocus>
        <texturenofocus colordiffuse="$INFO[Skin.String(BingieDetailsFlagColor)]" border="20">dialogs/keyboard/key-nf.png</texturenofocus>
    </include>

    <!-- Search Keyboard Row -->
    <include name="SearchKeyboardRow">
        <control type="grouplist" id="$PARAM[id]">
            <width>455</width>
            <height>search_key_size</height>
            <orientation>horizontal</orientation>
            <itemgap>-10</itemgap>
            <onleft>$PARAM[onleft]</onleft>
            <onright>$PARAM[onright]</onright>
            <include>$PARAM[row_include]</include>
        </control>
    </include>

