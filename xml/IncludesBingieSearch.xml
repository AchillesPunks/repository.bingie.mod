    <include name="Search_Suggestions_Container">
        <param name="visibility" default="true" />
        <param name="left" default="75" />
        <param name="onback" default="9000" />
        <param name="onup" default="101" />
        <param name="ondown" default="noop" />
        <param name="onright" default="77777" />
        <param name="onleft" default="900" />
        <definition>
            <control type="list" id="$PARAM[id]">
                <nested/>
                <visible>$PARAM[visibility]</visible>
                <left>$PARAM[left]</left>
                <top>$PARAM[top]</top>
                <width>$PARAM[width]</width>
                <height>$PARAM[height]</height>
                <orientation>vertical</orientation>
                <onback>$PARAM[onback]</onback>
                <onup>$PARAM[onup]</onup>
                <ondown>$PARAM[ondown]</ondown>
                <onright>$PARAM[onright]</onright>
                <onleft>$PARAM[onleft]</onleft>
                <align>left</align>
                <itemlayout width="$PARAM[width]" height="65">
                    <control type="label">
                        <left>25</left>
                        <width>90%</width>
                        <height>100%</height>
                        <aligny>center</aligny>
                        <align>left</align>
                        <font>Reg30</font>
                        <textcolor>$INFO[Skin.String(BingiePrimaryTextColor)]</textcolor>
                        <scroll>false</scroll>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                </itemlayout>
                <focusedlayout width="$PARAM[width]" height="65">
                    <control type="group">
                        <left>10</left>
                        <control type="image">
                            <width>100%</width>
                            <height>100%</height>
                            <aligny>top</aligny>
                            <align>left</align>
                            <aspectratio align="left" aligny="top">stretch</aspectratio>
                            <colordiffuse>$VAR[ViewDetailsBorderFocusColor]</colordiffuse>
                            <texture colordiffuse="$VAR[SuggestionBorderColor]">bingie/border/default_button_fo.png</texture>
                            <visible>Control.HasFocus($PARAM[id]) | Control.IsVisible(500)</visible>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <width>90%</width>
                            <height>100%</height>
                            <aligny>center</aligny>
                            <align>left</align>
                            <font>Reg30</font>
                            <textcolor>$INFO[Skin.String(ViewDetailsTextColor)]</textcolor>
                            <scroll>false</scroll>
                            <label>$INFO[ListItem.Label]</label>
                        </control>
                        <control type="button">
                            <visible>false</visible>
                            <onfocus condition="String.IsEmpty(Skin.String(CustomSearchTerm))">$PARAM[onitemfocus]</onfocus>
                            <onfocus condition="String.IsEmpty(Skin.String(CustomSearchTerm))">$PARAM[onitemfocus2]</onfocus>
                        </control>
                    </control>
                </focusedlayout>
            </control>
        </definition>
    </include>

    <!-- Search Container Template -->
    <include name="SearchContainerTemplate">
        <definition>
            <control type="panel" id="$PARAM[id]">
                <nested />
                <visible>Container($PARAM[id]).IsUpdating | Integer.IsGreater(Container($PARAM[id]).NumItems,0) | $PARAM[force_visible]</visible>
                <visible>$PARAM[visibility]</visible>
                <width>$PARAM[width]</width>
                <height>$PARAM[height]</height>
                <orientation>$PARAM[orientation]</orientation>
                <pagecontrol>$PARAM[pagecontrol]</pagecontrol>
                <scrolltime tween="quadratic">400</scrolltime>
                <preloaditems>4</preloaditems>
                <itemlayout width="$PARAM[layoutwidth]" height="$PARAM[layoutheight]">
                    <control type="group">
                        <width>$PARAM[layoutwidth]</width>
                        <height>$PARAM[layoutheight]</height>
                        <include>$PARAM[layout]Layout</include>
                        <control type="group">
                            <include content="$PARAM[layout]Tags">
                                <param name="force_tags" value="$PARAM[force_tags]" />
                                <param name="widgetTags" value="$PARAM[widgetTags]" />
                            </include>
                            <include>New_Episodes_Tag_Overlay</include>
                        </control>
                    </control>
                </itemlayout>
                <focusedlayout width="$PARAM[layoutwidth]" height="$PARAM[layoutheight]">
                    <control type="group">
                        <width>$PARAM[layoutwidth]</width>
                        <height>$PARAM[layoutheight]</height>
                        <include>$PARAM[layout]LayoutFocus</include>
                        <control type="group">
                            <include content="$PARAM[layout]Tags">
                                <param name="force_tags" value="$PARAM[force_tags]" />
                                <param name="widgetTags" value="$PARAM[widgetTags]" />
                            </include>
                            <include condition="String.IsEqual(Skin.String(HomeLayout),bingie)">New_Episodes_Tag_Overlay</include>
                        </control>
                    </control>
                </focusedlayout>
            </control>
        </definition>
    </include>

    <!-- Tag Overlays -->
    <include name="ThumbsViewTags">
        <include condition="!String.IsEqual(Skin.String(Widgets_DisplayTags),disable) | $PARAM[force_tags]">ThumbsTagOverlay</include>
