                <align>left</align>
                <label>$LOCALIZE[525]</label>
                <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                <font>Reg30</font>
            </control>
            <control type="list" id="100">
                <!--List 100-->
