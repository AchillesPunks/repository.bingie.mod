<?xml version="1.0" encoding="utf-8"?>
<!-- BINGIE -->
<window>
    <defaultcontrol allways="true">50</defaultcontrol>
	
	<onunload>ClearProperty(ShowViewSubMenu,Home)</onunload>
    <views>50</views>
	<include>animation_window_open_close</include>
    <controls>
        <!--Music Background-->
        <include>GlobalBackground</include>
         <!--Header-->
        <include>Header</include>
         <!-- footer -->
		<control type="image">	
			<posx>50</posx>
			<posy>960</posy>
			<width>1820</width>
			<height>84</height>
			<texture colordiffuse="$INFO[Skin.String(FooterPanelColor)]">dialogs/footer_classic.png</texture>
			<visible>[!Skin.HasSetting(NoFooterBar) + Skin.HasTheme(classic)]</visible>
			<include>animation_fade_visible_hidden</include>				
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>982</posy>
			<width>100%</width>
			<height>110</height>
			<texture border="5">diffuse/panel.png</texture>
			<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
			<visible>!Skin.HasTheme(classic) + !Skin.HasSetting(nofooterbar)</visible>
			<include>animation_fade_visible_hidden</include>
		</control>

		<!--Panel-->
        <control type="image">
            <posx>60</posx>
            <posy>240</posy>
            <width>1800</width>
            <height>660</height>
            <texture border="5">diffuse/panel.png</texture>
			<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
        </control>
        <control type="group">
            <!--Start Left Panel-->
            <description>Left panel</description>
            <posx>20</posx>
            <posy>300</posy>
            <control type="image">
				<posx>80</posx>
				<posy>-20</posy>
				<width>850</width>
				<height>600</height>
				<texture border="5">diffuse/panel.png</texture>
				
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
			</control>
                 <control type="label">
                <!--Header-->
                <posx>150</posx>
                <width>700</width>
                <align>left</align>
                <label>$LOCALIZE[1217]</label>
                <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                <font>Reg30</font>
            </control>
            <control type="list" id="50">
                <!--List 50-->
                <posx>0</posx>
                <posy>100</posy>
                <width>950</width>
                <height>420</height>
                <onright>100</onright>
                <onup>50</onup>
                <ondown>50</ondown>
                <itemlayout height="69" width="900">
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                    </control>
                </itemlayout>
                <focusedlayout height="69" width="900">
                    <control type="image">
                        <visible>Control.HasFocus(50)</visible>
                        <posx>100</posx>
                        <posy>7</posy>
                        <width>850</width>
                        <height>50</height>
                        <colordiffuse>$INFO[Skin.String(ButtonFocusColor)]</colordiffuse>
                        <texture>diffuse/slider_big.png</texture>
                    </control>
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>$INFO[Skin.String(GeneralHighlightTextColor)]</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>$INFO[Skin.String(GeneralHighlightTextColor)]</textcolor>
                    </control>
                </focusedlayout>
            </control>
            <control type="label">
                <!--Object Info RSS-->
                <description>Header Label</description>
                <posx>150</posx>
                <posy>680</posy>
                <width>480</width>
                <align>left</align>
                <label>$INFO[Container(50).ListItem.Label]</label>
                <font>Reg30</font>
                <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
            </control>
            <control type="label">
                <!--Page Info RSS-->
                <posx>870</posx>
                <posy>680</posy>
                <width>250</width>
                <align>right</align>
                <font>Reg30</font>
                <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                <label>$INFO[Container(50).CurrentPage]/$INFO[Container(50).NumPages]</label>
            </control>
        </control>
        <control type="group">
            <!--Start Right Panel-->
            <description>Right panel</description>
            <posx>900</posx>
            <posy>300</posy>
            <control type="image">
				<posx>80</posx>
				<posy>-20</posy>
				<width>850</width>
				<height>600</height>
				<texture border="5">diffuse/panel.png</texture>
				
