<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>4K Movies</name>
    <match>all</match>
    <rule field="videoresolution" operator="greaterthan">
        <value>1080p</value>
    </rule>
	<order direction="ascending">sorttitle</order>
    <limit>200</limit>
</smartplaylist>