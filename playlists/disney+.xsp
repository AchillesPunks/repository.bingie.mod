<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="tvshows">
    <name>Disney+</name>
    <match>all</match>
    <rule field="studio" operator="contains">
        <value>disney+</value>
    </rule>
    <order direction="ascending">sorttitle</order>
    <limit>200</limit>
</smartplaylist>