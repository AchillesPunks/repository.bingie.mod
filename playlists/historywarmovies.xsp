<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>History &amp; War Movies</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>History</value>
        <value>War</value>
    </rule>
    <order direction="ascending">random</order>
    <limit>200</limit>
</smartplaylist>
