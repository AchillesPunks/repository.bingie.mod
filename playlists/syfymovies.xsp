<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>SyFy Movies</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>SyFy</value>
        <value>Science Fiction</value>
        <value>Science-Fiction</value>
    </rule>
    <order direction="ascending">random</order>
    <limit>200</limit>
</smartplaylist>
