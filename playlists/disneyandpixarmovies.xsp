<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>Disney &amp; Pixar Films</name>
    <match>all</match>
    <rule field="studio" operator="contains">
        <value>disney</value>
        <value>pixar</value>
    </rule>
    <order direction="ascending">sorttitle</order>
    <limit>200</limit>
</smartplaylist>