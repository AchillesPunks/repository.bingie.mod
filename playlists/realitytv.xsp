<!-- BINGIE -->
<smartplaylist type="tvshows">
    <name>Reality TV</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>Reality</value>
        <value>Reality TV</value>
        <value>Reality-TV</value>
        <value>Game Show</value>
    </rule>
    <order direction="ascending">random</order>
    <limit>200</limit>
</smartplaylist>
