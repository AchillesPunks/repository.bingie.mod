<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>In Progress Movies</name>
    <match>all</match>
    <rule field="inprogress" operator="true" />
    <limit>200</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
