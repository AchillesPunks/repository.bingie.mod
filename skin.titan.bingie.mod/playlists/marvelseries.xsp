<!-- BINGIE -->
<smartplaylist type="tvshows">
    <name>Marvel Series</name>
    <match>one</match>
    <rule field="title" operator="contains">
        <value>Marvel</value>
    </rule>
    <rule field="studio" operator="contains">
        <value>Marvel</value>
    </rule>
    <order direction="ascending">random</order>
    <limit>200</limit>
</smartplaylist>